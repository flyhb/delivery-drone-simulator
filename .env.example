# RPC endpoint for the target network (e.g. local Hardhat node or Berachain)
RPC_URL=http://localhost:8545

# Chain ID of the network (e.g. 31337 for Hardhat, 80069 for Bepolia)
CHAIN_ID=31337

# Address of the ioIDStore contract on your network
IOID_STORE=0x0000000000000000000000000000000000000000

# Optional, only if you want to override what's in ioIDStore for some reason.
# Address of the ioIDRegistry contract on your network
IOID_REGISTRY=0x0000000000000000000000000000000000000000

# Hummingbird liveness contract 
HUMMINGBIRD=0x0000000000000000000000000000000000000000

# Optional: owner private key used to pay the registration fee. Leave empty to be prompted at runtime
OWNER_PRIVATE_KEY=


# Optional: initial coordinates and ready flag for first heartbeat
# If unset, the simulator will use demo defaults (Boston coords, ready=true)
INIT_LAT_E7=377749000         # 37.7749° × 1e7
INIT_LON_E7=-1224194000       # -122.4194° × 1e7
INIT_READY=1                  # 1 = ready, 0 = not ready

# Heartbeat cadence (milliseconds). Default: 60000
HEARTBEAT_MS=60000
                # 1 = ready, 0 = not ready
