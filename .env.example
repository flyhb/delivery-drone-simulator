# RPC endpoint for the target network (e.g. local Hardhat node or Berachain)
RPC_URL=http://localhost:8545

# Chain ID of the network (e.g. 31337 for Hardhat, 80069 for Bepolia)
CHAIN_ID=31337

# Address of the ioIDRegistry contract on your network
IOID_REGISTRY=0x0000000000000000000000000000000000000000

# Price of an ioID registration in ETH (will be converted using parseEther)
IOID_PRICE=1.0

# Address of the Device NFT contract representing your physical device
DEVICE_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Token ID of the device NFT to register (as a string)
DEVICE_TOKEN_ID=1

# Optional: owner private key used to pay the registration fee. Leave empty to be prompted at runtime
OWNER_PRIVATE_KEY=